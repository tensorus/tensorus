# Development environment configuration for Tensorus
# This file contains safe-to-commit development settings

# API Authentication
TENSORUS_AUTH_ENABLED=true
TENSORUS_API_KEYS=tsr_dev_key_12345678901234567890123456789012345678

# Storage  
TENSORUS_STORAGE_BACKEND=in_memory

# Security
TENSORUS_AUDIT_LOG_PATH=dev_audit.log

# Development settings
TENSORUS_MINIMAL_IMPORT=1
NQL_USE_LLM=false

# Web security (relaxed for development)
TENSORUS_X_FRAME_OPTIONS=SAMEORIGIN
TENSORUS_CONTENT_SECURITY_POLICY=default-src 'self' 'unsafe-inline' 'unsafe-eval'; connect-src 'self' https: